        file.write('task dft '+task+'\n\n')
elif ( method == "pbe0" ):
        print_dft(file,method,cluster,basis)
        file.write('task dft '+task+'\n\n')
else:
        print "WARNING: <method> not recognized.  It is being treated as a DFT functional."
        print_dft(file,method,cluster,basis)
        file.write('task dft '+task+'\n\n')
file.close()
